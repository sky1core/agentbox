{
  "name": "@sky1core/agentbox",
  "version": "1.0.0",
  "description": "Lima VM launcher for AI coding agents",
  "type": "module",
  "bin": {
    "agentbox": "dist/index.js"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json && chmod +x dist/index.js",
